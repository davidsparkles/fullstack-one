{
  "name": "fullstack.one-example",
  "version": "0.1.0",
  "description": "Example project: how to use fullstack.one",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "main": "index.ts",
  "scripts": {
    "start": "ts-node .",
    "debug": "FULLSTACK_ONE_DEV=true ts-node-dev --notify=false --inspect=9229 .",
    "dev": "FULLSTACK_ONE_DEV=true ts-node-dev --notify=false .",
    "dev-pure": "FULLSTACK_ONE_DEV=true ts-node .",
    "tslint": "tslint './server/**/*.ts?(x)' -e '**/node_modules/**' --fix",
    "test": "mocha -r ts-node/register test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstack-build/fullstack.one-example.git"
  },
  "keywords": [
    "fullstack.one",
    "example"
  ],
  "author": "Eugene <opensource@terehov.de> (http://fullstack.build)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullstack-build/fullstack.one-example/issues"
  },
  "homepage": "https://github.com/fullstack-build/fullstack.one-example#readme",
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/node": "^8.0.57",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "ts-node": "^4.1.0",
    "ts-node-dev": "^1.0.0-pre.26",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0"
  }
