  "description": "PostgreSQL, Node.js, GraphQL, TypeScript, API Framework",
  "engines": {
    "node": ">=8.6"
  },
  "main": "packages/server/main",
  "scripts": {
    "start": "echo \"Error: no start specified\" && exit 1",
    "tslint": "tslint --project '.' --fix",
    "test": "mocha -r ts-node/register test/*.ts",
