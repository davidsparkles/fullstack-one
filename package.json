    "url": "https://github.com/fullstack-build/fullstack.one/issues"
  },
  "homepage": "https://github.com/fullstack-build/fullstack.one#readme",
  "devDependencies": {
    "@types/node": "^8.0.31",
    "git-hooks": "^1.1.9",
    "lerna": "^2.9.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0"
  },
  "dependencies": {
    "npm": "^5.6.0",
    "ts-node": "^3.3.0",
