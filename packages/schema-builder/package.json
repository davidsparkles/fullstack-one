{
  "name": "@fullstack-one/schema-builder",
  "version": "0.5.7",
  "description": "fullstack.one schema builder package",
  "main": "index.js",
  "types": "lib/index.ts",
  "scripts": {
    "test": "ava --verbose ./test/**/*test.ts",
    "coverage": "nyc --reporter=html --reporter=text ava --verbose ./test/**/*test.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstack-build/fullstack.one.git"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@fullstack-one/auth": "^0.5.5",
    "@fullstack-one/boot-loader": "^0.5.5",
    "@fullstack-one/config": "^0.5.5",
    "@fullstack-one/db": "^0.5.5",
    "@fullstack-one/di": "^0.5.2",
    "@fullstack-one/helper": "^0.5.5",
    "@fullstack-one/logger": "^0.5.5",
    "@types/graphql": "^0.13.4",
    "@types/lodash": "^4.14.120",
    "deep-diff": "^1.0.0",
    "deep-equal": "^1.0.1",
    "deepmerge": "^2.0.1",
    "fast-glob": "^2.2.1",
    "graphql": "^0.13.2",
    "lodash": "^4.17.5",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.10"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
