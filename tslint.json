{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "no-namespace": false,
    "object-literal-sort-keys": false,
    "max-line-length": [true, 150]
  },
  "rulesDirectory": [],
  "linterOptions":{
    "exclude":  ["**/node_modules/**", "*.d.ts"]
  }
}
